<sw-card-view style="position: relative;">
    <sw-card class="sw-card--grid"
             :title="$tc('noval-payment.settingForm.paymentSettings.creditcard.cardTitle')">
             
        {% block noval_payment_content_card_channel_config_payment_card_container %}
			<sw-container>
				{% block noval_payment_content_card_channel_config_payment_card_container_settings %}
					<div v-if="actualConfigData" class="noval_payment-settings-credentials">
						{% block noval_payment_content_card_channel_config_payment_card_container_settings_creditcard %}
							<sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.creditcard.testMode']"
                                                :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.creditcard.testMode']"
                                                >
                                <template #content="props">
                                    <sw-switch-field name="NovalnetPayment.settings.creditcard.testMode"
													 bordered
                                                   :mapInheritance="props"
                                                   :label="$tc('noval-payment.settingForm.paymentSettings.testmode.label')"
                                                   :helpText="$tc('noval-payment.settingForm.paymentSettings.testmode.tooltipText')"
                                                   :value="props.currentValue"
                                                   @change="props.updateCurrentValue">
                                    </sw-switch-field>
                                </template>
                            </sw-inherit-wrapper>
                            <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.creditcard.cc3D']"
                                                :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.creditcard.cc3D']"
                                                >
                                <template #content="props">
                                    <sw-switch-field name="NovalnetPayment.settings.creditcard.cc3D"
													 bordered
													 :mapInheritance="props" :label="$tc('noval-payment.settingForm.paymentSettings.creditcard.cc3D.label')" :helpText="$tc('noval-payment.settingForm.paymentSettings.creditcard.cc3D.tooltipText')"
													 :value="props.currentValue"
													 @change="props.updateCurrentValue">
                                    </sw-switch-field>
                                </template>
                            </sw-inherit-wrapper>
                            <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.creditcard.forcecc3D']"
                                                :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.creditcard.forcecc3D']"
                                                >
                                <template #content="props">
                                    <sw-switch-field name="NovalnetPayment.settings.creditcard.forcecc3D"
													 bordered
													 :mapInheritance="props" :label="$tc('noval-payment.settingForm.paymentSettings.creditcard.forcecc3D.label')" :helpText="$tc('noval-payment.settingForm.paymentSettings.creditcard.forcecc3D.tooltipText')"
													 :value="props.currentValue"
													 @change="props.updateCurrentValue">
                                    </sw-switch-field>
                                </template>
                            </sw-inherit-wrapper>
