                                                :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.creditcard.onHold']"
												:label="$tc('noval-payment.settingForm.paymentSettings.onHold.label')">
                                <template #content="props">
                                    <sw-single-select name="NovalnetPayment.settings.creditcard.onHold"
													  labelProperty="name"
													  valueProperty="id"
													  :options="onholdOptions"
													  :isInherited="props.isInherited"
													  :value="props.currentValue == null ? selected : props.currentValue"
													  @change="props.updateCurrentValue">
									</sw-single-select>
                                </template>
                            </sw-inherit-wrapper>
                            <sw-inherit-wrapper v-model="actualConfigData['NovalnetPayment.settings.creditcard.onHoldAmount']"
                                                :inheritedValue="selectedSalesChannelId == null ? null : allConfigs['null']['NovalnetPayment.settings.creditcard.onHoldAmount']"
